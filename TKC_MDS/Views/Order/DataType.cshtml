<script>
	var IS_NEW_TYPE=true;

	const cancle = ()=>{
		window.location.href = "/order/datatype";
	}

	const checkDataType=(value)=>{
		let same_value_counter = 0;
		document.getElementById("select_body").querySelectorAll(`tr > td > input[id="input_checker"]`).forEach(input => {
			if(input.value == value && value != ""){
				same_value_counter++;
				input.style.outline = "3px solid red"; 
			}else{
				input.style.outline = "none"; 
			}
		});

		if(same_value_counter > 1){
			Swal.fire('ฟิวล์ นี้ถูกใช้งานไปแล้ว');
		}
	}

	const selectChangeField=(value)=>{

		if(value == "false"){ //ถ้าไม่มีตัวขั้น
			document.getElementById("Seperator").querySelectorAll("option").forEach(option => {
			if(option.value == ""){
				option.selected = true;

			}else{
				option.selected = false;
			}
			});
			document.getElementById("Seperator").disabled = true; //lock seperater
		}else{
			document.getElementById("Seperator").disabled = false; //unlock seperater
		} 

		document.querySelectorAll("#input_seperater").forEach(input => { //set readOnly for none seperater
			
				input.readOnly = ! input.readOnly;
		});
	}

	const clickCheckBox=(checkbox)=>{
		if(checkbox.checked){
			checkbox.checked = true;
			checkbox.value = "true";
		}else{
			checkbox.checked = false;
			checkbox.value = "false";
		}
	}

	const showDialogAddUser = ()=>
	{
		let modal = document.getElementById("modal_add_user");
		modal.classList.toggle("show")
		if(modal.classList.contains("show")){
			modal.style.display = "block";

		}else{
			modal.style.display = "none";
		}
	}

	const haveManyDue = (value)=>{
		if (value == "true"){
			document.querySelectorAll("#has_many_due").forEach(el => { el.style.display = ""; });
		}else{
			document.querySelectorAll("#has_many_due").forEach(el => { el.style.display = "none"; });
		}
	}

	const checkSelectChange=(value)=>{

		let same_value_counter = 0;
		document.getElementById("select_body").querySelectorAll("select").forEach(select =>{
		if (value === select.value && value!=="" ) {
			same_value_counter++;
		}
		if(same_value_counter > 1){
			Swal.fire('ฟิวล์ นี้ถูกใช้งานไปแล้ว');
		}

		});
	}

	window.onload = async () => {

createField(20);


		const selectCustumer = document.getElementById("custumer_select");
		const listUser = await fetch("/order/JsonCustumer");
		const users_json = await listUser.json();
		selectCustumer.innerHTML = users_json.reduce((prev, curr) => prev += `<option value="${curr?.custID}">${curr?.custID}-${curr?.custName} </option>`, `<option selected value="" >---เลือก---</option>`);
		
		const dataType = await fetch("/order/JsonDataType_MS");
		const data_type = await dataType.json();
		const select_data_type = document.getElementById("select_data_type");
		select_data_type.innerHTML = data_type.reduce((prev, curr) => prev += `<option value="${curr?.typeCode}">${curr?.typeCode}-${curr?.typeName}</option>`, `<option selected value="" >---เลือก---</option>`);
		const modal_data_type = document.getElementById("data_type");
		modal_data_type.innerHTML = data_type.reduce((prev, curr, index) => prev += `							
								<tr>
										<td class="text-start  pe-3 min-w-10px">${index+1}</td>
										<td class="text-start  pe-3 min-w-100px">${curr?.typeName}</td>
